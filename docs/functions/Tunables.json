{
  "$schema": "https://raw.githubusercontent.com/sims4toolkit/misc/main/json-schemas/docs-item-schema.json",
  "imports": [
    "import { T, U, ... } from \"@s4tk/tunables\"; // ESM",
    "const { T, U, ... } = require(\"@s4tk/tunables\"); // CJS"
  ],
  "header": {
    "title": {
      "name": "Tunable Builders"
    },
    "description": "Helper functions for creating tunable nodes.",
    "repoUrl": "https://github.com/sims4toolkit/tunables/blob/main/src/tunables.ts"
  },
  "sections": [
    {
      "title": "Functions",
      "content": [
        {
          "displayType": "function",
          "name": "C",
          "returnType": {
            "name": "XmlNode",
            "path": {
              "pkg": "xml-dom",
              "group": "interfaces",
              "item": "XmlNode"
            }
          },
          "description": "Creates and returns a class tunable (C tag).",
          "arguments": [
            {
              "name": "args",
              "type": {
                "name": "object"
              },
              "description": "Object containing node values.",
              "optional": false,
              "properties": [
                {
                  "name": "name",
                  "description": "Value to put in the 'n' attribute.",
                  "optional": false,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "children",
                  "description": "Child nodes to put in the class.",
                  "optional": true,
                  "type": {
                    "name": "XmlNode[]",
                    "path": {
                      "pkg": "xml-dom",
                      "group": "interfaces",
                      "item": "XmlNode"
                    }
                  }
                }
              ]
            }
          ],
          "examples": [
            {
              "code": "const classNode = C({\n  name: \"SomeClass\",\n  children: [\n    T({\n      name: \"SOME_VALUE\",\n      value: 10\n    })\n  ]\n});"
            },
            {
              "lang": "xml",
              "code": "<!-- Output -->\n<C n=\"SomeClass\">\n  <T n=\"SOME_VALUE\">10</T>\n</C>"
            }
          ]
        },
        {
          "displayType": "function",
          "name": "E",
          "returnType": {
            "name": "XmlNode",
            "path": {
              "pkg": "xml-dom",
              "group": "interfaces",
              "item": "XmlNode"
            }
          },
          "description": "Creates and returns an enum tunable (E tag).",
          "arguments": [
            {
              "name": "args",
              "type": {
                "name": "object"
              },
              "description": "Object containing node values.",
              "optional": false,
              "properties": [
                {
                  "name": "name",
                  "description": "Value to put in the 'n' attribute.",
                  "optional": true,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "value",
                  "description": "Value to put in this node.",
                  "optional": true,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "comment",
                  "description": "Comment to put in this node.",
                  "optional": true,
                  "type": {
                    "name": "string"
                  }
                }
              ]
            }
          ],
          "examples": [
            {
              "code": "const enumNode = E({\n  name: \"age\",\n  value: \"ADULT\",\n  comment: \"This is for adults.\"\n});"
            },
            {
              "lang": "xml",
              "code": "<!-- Output -->\n<E n=\"age\">ADULT<!--This is for adults.--></E>"
            }
          ]
        },
        {
          "displayType": "function",
          "name": "I",
          "returnType": {
            "name": "XmlNode",
            "path": {
              "pkg": "xml-dom",
              "group": "interfaces",
              "item": "XmlNode"
            }
          },
          "description": "Creates and returns an instance tunable (I tag).",
          "arguments": [
            {
              "name": "args",
              "type": {
                "name": "object"
              },
              "description": "Object containing node values.",
              "optional": false,
              "properties": [
                {
                  "name": "c",
                  "description": "Value to put in the 'c' attribute.",
                  "optional": false,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "i",
                  "description": "Value to put in the 'i' attribute.",
                  "optional": false,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "m",
                  "description": "Value to put in the 'm' attribute.",
                  "optional": false,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "n",
                  "description": "Value to put in the 'n' attribute.",
                  "optional": false,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "s",
                  "description": "Value to put in the 's' attribute.",
                  "optional": false,
                  "type": {
                    "name": "string | number | bigint"
                  }
                },
                {
                  "name": "children",
                  "description": "Children to add to this node.",
                  "optional": true,
                  "type": {
                    "name": "XmlNode[]",
                    "path": {
                      "pkg": "xml-dom",
                      "group": "interfaces",
                      "item": "XmlNode"
                    }
                  }
                }
              ]
            }
          ],
          "examples": [
            {
              "code": "const inst = I({\n  c: \"Trait\",\n  i: \"trait\",\n  m: \"traits.trait\",\n  n: \"SomeNewTrait\",\n  s: fnv64(\"SomeNewTrait\"),\n  children: [...]\n});"
            },
            {
              "lang": "xml",
              "code": "<!-- Output -->\n<I c=\"Trait\" i=\"trait\" m=\"traits.trait\" n=\"SomeNewTrait\" s=\"4129886381790487547\">\n  ...\n</I>"
            }
          ]
        }
      ]
    }
  ]
}