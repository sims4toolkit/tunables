{
  "$schema": "https://raw.githubusercontent.com/sims4toolkit/misc/main/json-schemas/docs-item-schema.json",
  "imports": [
    "import { L } from \"@s4tk/tunables\"; // ESM",
    "const { L } = require(\"@s4tk/tunables\"); // CJS"
  ],
  "header": {
    "title": {
      "name": "Tunable List Builder"
    },
    "description": "Helper function for creating a tunable list node (<L> tag).",
    "repoUrl": "https://github.com/sims4toolkit/tunables/blob/main/src/tunables.ts"
  },
  "sections": [
    {
      "content": [
        {
          "displayType": "function",
          "name": "L",
          "returnType": {
            "name": "XmlElementNode",
            "path": {
              "pkg": "xml-dom",
              "group": "classes",
              "item": "XmlElementNode"
            }
          },
          "description": "Creates and returns a list tunable (<L> tag).",
          "arguments": [
            {
              "name": "args",
              "type": {
                "name": "object"
              },
              "description": "Object containing node values.",
              "optional": false,
              "properties": [
                {
                  "name": "name",
                  "description": "Value to put in the 'n' attribute.",
                  "optional": true,
                  "type": {
                    "name": "string"
                  }
                },
                {
                  "name": "children",
                  "description": "Children to put in this node.",
                  "optional": true,
                  "type": {
                    "name": "XmlNode[]",
                    "path": {
                      "pkg": "xml-dom",
                      "group": "interfaces",
                      "item": "XmlNode"
                    }
                  }
                }
              ]
            }
          ],
          "examples": [
            {
              "code": "const listNode = L({\n  name: \"some_list\",\n  children: [\n    T({ value: 1 }),\n    T({ value: 2 })\n  ]\n});"
            },
            {
              "lang": "xml",
              "code": "<!-- Output -->\n<L n=\"some_list\">\n  <T>1</T>\n  <T>2</T>\n</L>"
            }
          ]
        }
      ]
    }
  ],
  "footer": {
    "seeAlso": [
      {
        "name": "T",
        "path": {
          "pkg": "tunables",
          "group": "nodes",
          "item": "T"
        }
      }
    ]
  }
}